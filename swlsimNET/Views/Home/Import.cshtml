@{
    ViewData["Title"] = "Import";
}
<div class="col-xs-12" style="height:110px;"></div>
<div>

    <h1>Import Character Stats</h1>

    <div class="col-md-3" style="margin: 10px 0px 10px 0px">

        <h5><b>Primary Weapon</b></h5>
        <form>
            <select v-model="form.primaryWeapon" id="primaryWeapon">
                <option>Blade</option>
                <option>Hammer</option>
                <option>Fist</option>
                <option>Blood</option>
                <option>Chaos</option>
                <option>Elemental</option>
                <option>Shotgun</option>
                <option>Pistols</option>
                <option>Assault Rifle</option>
            </select>

            <h5><b>Secondary Weapon</b></h5>

            <select v-model="form.secondaryWeapon" class="form-control select2">
                <option>Blade</option>
                <option>Hammer</option>
                <option>Fist</option>
                <option>Blood</option>
                <option>Chaos</option>
                <option>Elemental</option>
                <option>Shotgun</option>
                <option>Pistols</option>
                <option>Assault Rifle</option>
            </select>

            <label>Combat Power</label>
            <input v-model="form.combatPower" class="form-control">
            <label>Critical Chance</label>
            <input v-model="form.criticalChance" class="form-control">
            <label>Critical Power</label>
            <input v-model="form.criticalPower" class="form-control">
            <label>Basic Signet</label>
            <input v-model="form.basicSignet" class="form-control">
            <label>Power Signet</label>
            <input v-model="form.powerSignet" class="form-control">
            <label>Elite Signet</label>
            <input v-model="form.eliteSignet" class="form-control">
            <div class="col-xs-12" style="height:15px;"></div>
            <button class="btn btn-default waves-effect waves-light">Start Simulation</button>
        </form>
    </div>
    <div class="col-md-8">
        <div class="col-xs-12" style="height:21px;"></div>
        <label>Action Priority List</label>
<textarea v-model="form.apl" class="form-control" rows="10">
</textarea>
        <div class="col-xs-12" style="height:8px;"></div>
        <input id="checkbox0" type="checkbox" checked>
        <label for="checkbox0">Opening Shot every ~ 20 seconds.</label>
        <br>
        <input id="checkbox0" type="checkbox" checked>
        <label for="checkbox0">Exposed with 100% uptime</label>
        <br>
        <input id="checkbox0" type="checkbox">
        <label for="checkbox0">Head signet is CDR</label>
    </div>
</div>