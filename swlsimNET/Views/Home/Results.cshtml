

@using System.Reflection
@using swlsimNET.ServerApp.Models
@*@model List<swlsimNET.ServerApp.Models.TablePopulator>*@
@{
    ViewData["Title"] = "Results";
}


<div class="col-xs-12" style="height:110px;"></div>
<div>
    @{
        var table = new TablePopulator();

        //    foreach (var name in table)
        //{
        //    name = table.Name;
        //}

    }
    <h1>
      DPS:
    </h1>
    <p>Here is some data:</p>

    <div class="row">
        <div class="col-lg-6">
            <div class="card-box">
                <h4 class="m-t-0 header-title"><b>DPS Timeline</b></h4>
                <canvas id="lineChart" height="200"></canvas>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="card-box">
                <h4 class="m-t-0 header-title"><b>% DMG Breakdown</b></h4>
                <canvas id="pie" height="200"></canvas>
            </div>
        </div>
    </div>
    <div class="col-lg-12">
        <h4 class="m-t-0 header-title"><b>Spell Breakdown</b></h4>
        <div class="p-20">
            <table class="table table table-hover m-0">
                <thead>
                <tr>
                    <th>Spell</th>
                    <th>DPS</th>
                    <th>DPS%</th>
                    <th>Executes</th>
                    <th>DPE</th>
                    <th>Type</th>
                    <th>Count</th>
                    <th>Avarage</th>
                    <th>Crit%</th>
                </tr>
                </thead>
                <tbody>
                <tr>

                    <th scope="row">@table.Name</th>
                    <td>@table.DamagePerSecond</td>
                    <td>@table.DpsPercentage</td>
                    <td>@table.Executes</td>
                    <td>@table.DamagePerExecution</td>
                    <td>@table.SpellType</td>
                    <td>@table.Count</td>
                    <td>@table.Avarage</td>
                    <td>@table.CritChance>
                </tr>
                <tr>
                    <th scope="row">Demolish</th>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                    <td>6</td>
                    <td>6</td>
                    <td>6</td>
                    <td>6</td>
                    <td>100%</td>
                </tr>
                <tr>
                    <th scope="row">Total</th>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                    <td>6</td>
                    <td>6</td>
                    <td>6</td>
                    <td>6</td>
                    <td>100%</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-lg-12">
        <h4 class="m-t-0 header-title"><b>Buff Breakdown</b></h4>
        <div class="p-20">
            <table class="table table table-hover m-0">
                <thead>
                <tr>
                    <th>Buff</th>
                    <th>Start</th>
                    <th>Refresh</th>
                    <th>Interval</th>
                    <th>Executes</th>
                    <th>Count</th>
                    <th>Uptime%</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <th scope="row">Smash</th>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                </tr>
                <tr>
                    <th scope="row">Demolish</th>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                </tr>
                <tr>
                    <th scope="row">Total</th>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                    <td>3</td>
                    <td>4</td>
                    <td>6</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
